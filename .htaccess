<FilesMatch "\.(ico|pdf|flv|jpg|jpeg|png|gif|js|css|swf)(\.gz)?$">
	Header set Expires "Thu, 15 Apr 2013 20:00:00 GMT"
	Header unset ETag
	FileETag None
</FilesMatch>

RewriteEngine on

RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*) index.php
